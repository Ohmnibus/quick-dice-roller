<?xml version="1.0" encoding="UTF-8"?>
<selector
	android:dither="true"
	xmlns:android="http://schemas.android.com/apk/res/android">
	<item
		android:state_checked="true"
		android:drawable="@drawable/context_rail_slider_btn_on" />
	<item
		android:state_window_focused="false"
		android:drawable="@drawable/context_rail_slider_btn_normal" />
	<item
		android:state_pressed="true"
		android:drawable="@drawable/context_rail_slider_btn_pressed" />
	<item
		android:state_focused="true"
		android:drawable="@drawable/context_rail_slider_btn_selected" />
	<item
		android:drawable="@drawable/context_rail_slider_btn_normal" />
</selector>