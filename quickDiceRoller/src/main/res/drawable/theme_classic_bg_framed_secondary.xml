<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
	<item>
		<shape
			android:shape="rectangle">
			<solid
				android:color="@color/classic_frame_secondary_solid_color"
				/>
		</shape>
	</item>
	<item>
		<shape
			android:shape="rectangle">
			<solid
				android:color="@null"
				/>
			<stroke
				android:width="@dimen/frame_border_size"
				android:color="@color/classic_frame_border_color"
				/>
			<padding
				android:left="@dimen/frame_padding_size"
				android:top="@dimen/frame_padding_size"
				android:right="@dimen/frame_padding_size"
				android:bottom="@dimen/frame_padding_size"
				/>
		</shape>
	</item>
</layer-list>